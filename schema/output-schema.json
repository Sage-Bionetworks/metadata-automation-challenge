{
    "$schema" : "http://json-schema.org/draft-07/schema",
    "title" : "Submission Outputs",
    "type" : "object",
    "description" : "Structure and formatting requirements for the output of metadata annotation tools submitted to the challenge",
    "properties" : {
        "columns": {
            "type": "array",
            "items": {
                "type": "object",
                "required": [
                    "columnNumber",
                    "headerValue",
                    "results"
                ],
                "properties": {
                    "columnNumber": {
                        "type": "integer",
                        "description": "The 1-indexed column number from the input table",
                        "minimum": 1
                    },
                    "headerValue": {
                        "type": "string",
                        "description": "The header value from the input table"
                    },
                    "results": {
                        "type": "array",
                        "description": "Annotation details (data element, data element concepts, observed values) for a selected column",
                        "items": {
                            "type": "object",
                            "required": [
                                "resultNumber",
                                "result",
                                "observedValues"
                            ],
                            "description": "Results",
                            "properties": {
                                "resultNumber": {
                                    "type": "integer",
                                    "description": "Rank of predicted match to be evaluated",
                                    "minimum": 1
                                },
                                "result": {
                                    "type": "object",
                                    "required": [
                                        "dataElement",
                                        "dataElementConcept"
                                    ],
                                    "description": "Annotation details (data element, data element concepts, observed values) for a selected column and current result",
                                    "properties": {
                                        "dataElement": {
                                            "type": "object",
                                            "required": [
                                                "id",
                                                "name"
                                            ],
                                            "description": "caDSR public ID and long name for a selected data element",
                                            "properties": {
                                                "id": {
                                                    "type": ["number", "null"],
                                                    "description": "The unique identifier of the common data element (CDE) in the caDSR",
                                                    "examples": [
                                                        3701084
                                                    ]
                                                },
                                                "name": {
                                                    "type": "string",
                                                    "description": "The descriptive set of words by which a CDE is known; constructed from a terminologyâ€™s preferred names for the DEC and value domain",
                                                    "examples": [
                                                        "APGAR Test Breathing Effort Score"
                                                    ]
                                                }
                                            }
                                        },
                                        "dataElementConcept": {
                                            "type": "object",
                                            "required": [
                                                "id",
                                                "name",
                                                "conceptCodes"
                                            ],
                                            "description": "caDSR public ID and long name for a selected data element concept",
                                            "properties": {
                                                "id": {
                                                    "type": ["number", "null"],
                                                    "description": "The unique identifier in the caDSR of the data element concept (DEC)",
                                                    "examples": [
                                                        2958442
                                                    ]
                                                },
                                                "name": {
                                                    "type": "string",
                                                    "description": "A unique combination of the data element Object Class and Property vocabulary names used to represent the concept",
                                                    "examples": [
                                                        "Assessment Multiple Birth"
                                                    ]
                                                },
                                                "conceptCodes": {
                                                    "type": "array",
                                                    "items": {
                                                        "type": "string"
                                                    },
                                                    "examples": [
                                                        "ncit:C70699", "ncit:C25364"
                                                    ]
                                                }
                                            }
                                        }
                                    }
                                },
                                "observedValues": {
                                    "type": "array",
                                    "description": "Mapping of unique observed values in rows to matched concepts",
                                    "items": {
                                        "type": "object",
                                        "required": [
                                            "rowValue",
                                            "permissibleValue"
                                        ],
                                        "properties": {
                                            "rowValue": {
                                                "type": ["string", "number", "null"],
                                                "examples": [
                                                    "900_00_1961"
                                                ]
                                            },
                                            "permissibleValue": {
                                                "type": "object",
                                                "required": [
                                                    "value",
                                                    "conceptCode"
                                                ],
                                                "properties": {
                                                    "value": {
                                                        "type": "string",
                                                        "examples": [
                                                            "CONFORMING", "NONCONFORMING"
                                                        ]
                                                    },
                                                    "conceptCode": {
                                                        "type": [
                                                            "string", "null"
                                                        ],
                                                        "example": null
                                                    }
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "additionalProperties" : false
}